{
  "manifest_version": 2,
  "version": "2.6",
  "name": "gr",
  "theme": {
    "images" : {
      "theme_frame" : "background-noise-toolbar.png",
    //   "theme_frame_overlay" : "images/theme_frame_stripe.png",
      "theme_toolbar" : "blah.png"
